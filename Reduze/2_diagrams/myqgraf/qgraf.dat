*output = 'ggtt_tree.qgraf.yaml' ;
output = 'ggtt_1loop.qgraf.yaml' ;
style = 'reduze.sty' ;
model = 'EW_reduze';

in =gluon[p1],gluon[p2];
out=top[p3],tbar[p4];
*loops=0;
loops=1;
loop_momentum = k;
options =  onshell, notadpole;

* Limit the number of gluon propagators to 1.

true = iprop[ gluon, 1, 1] ;
